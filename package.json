{
  "name": "phaser-starter-template",
  "version": "0.1.0",
  "scripts": {
    "eslint": "eslint .",
    "prettier": "prettier --write ./src/.",
    "dev:server": "nodemon ./server/index.ts",
    "dev:client": "webpack serve -c ./client/webpack.config.js --env phaser-debug=true",
    "dev": "concurrently -c \"bgBlue.bold,bgMagenta.bold\" \"npm:dev:server\" \"npm:dev:client\"",
    "build:server": "tsc --project ./server",
    "build:client": "webpack -c ./client/webpack.config.js --env phaser-debug=false --env production=true",
    "start": "node ./build/index.js",
    "generate": "node ./scripts/generate.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "clean-webpack-plugin": "*",
    "concurrently": "^6.4.0",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "generate-template-files": "^3.2.0",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.15",
    "prettier": "2.3.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.4.0",
    "typecript": "*",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.4.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "phaser": "^3.55.2"
  }
}